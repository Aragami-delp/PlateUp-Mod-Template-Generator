{
    "$schema": "http://json.schemastore.org/template",
    "author": "Aragami",
    "classifications": ["PlateUp", "Mod", "Workshop", "KitchenLib", "Harmony"],
    "identity": "AragamiPlateUpModTemplate",
    "name": "PlateUp Workshop template with KitchenLib",
    "shortName": "plateupworkshopmod",
    "preferNameDirectory": true,
    "sourceName": "ModName",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "defaultName": "Unnamed_PlateUp_Mod",
    "symbols": {
        "guid": {
            "type": "parameter",
            "defaultValue": "your.guid",
            "replaces": "example.guid",
            "isRequired": true
        },
        "author": {
            "type": "parameter",
            "defaultValue": "yourName",
            "replaces": "username",
            "isRequired": true
        },
        "plateupgamedirectory": {
            "type": "parameter",
            "defaultValue": "C:\\Program Files (x86)\\Steam\\SteamLibrary\\steamapps\\common\\PlateUp\\PlateUp",
            "replaces": "gamepath",
            "isRequired": true
        }
    }
}